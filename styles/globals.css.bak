:root {
    --white: #fff;
    --red-light: #fedcdb;
    --grey-light: #f0f1f2;
    --red: #ff0000;
    --black: #000;
    --black-light: #333;
}

p {
    font-size: 14px;
}

.logo {
    /* width: 110px; */
    height: 28px;
}

.tableFontsize {
    font-size: 14px !important;
    text-transform: capitalize !important;
    line-height: 1 !important;
}

.MuiListItemIcon-root {
    min-width: auto !important;
}

.redBadge {
    background-color: var(--red-light) !important;
    color: var(--red) !important;
}

.greyBadge {
    background-color: var(--grey-light) !important;
    color: var(--black) !important;
}

.MuiCardContent-root:last-child {
    padding-bottom: 0px;
}

.MuiCard-root {
    box-shadow: none;
}

.slideritem.MuiCardContent-root:last-child {
    padding-bottom: 0;
}

.carouselitem {
    background-color: var(--white);
    color: var(--black);
    width: 45px;
    height: 45px;
    padding: 5px;
}

.textCapitalize {
    text-transform: capitalize;
}

.MuiDivider-light {
    border-color: #d0d1d2;
}

.MuiCard-root {
    border-radius: 8px;
}

.MuiBottomNavigationAction-label {
    color: var(--black);
    font-size: 12px;
    margin-top: 3px;
}

.MuiBottomNavigationAction-label.Mui-selected {
    font-size: 12px;
}

.MuiDialog-paper:not(.MuiDialog-paperFullScreen) {
    margin: 20px;
    width: 100%;
}

.MuiTabPanel-root:first-child {
    padding: 0;
}

.MuiTable-root tr:last-child>td {
    border: 0;
}

.racingTable td {
    height: 50px;
}

/* .racingTable td:not(:last-child)>* {
	font-size: 12px;
} */

.racingTable .MuiSvgIcon-root {
    font-size: 20px !important;
}

.RacingTabs .MuiTab-root {
    border-radius: 30px;
    padding: 8px 11px;
    min-height: 20px;
    margin-right: 10px;
    min-width: 80px;
}

.RacingTabs {
    min-height: 40px;
    align-items: center;
}

.TipsterTabs .MuiTab-root {
    padding: 8px;
    min-width: 75px;
}

.textAlignCoulmnCenter {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}

.MuiSvgIcon-colorAction {
    color: var(--black);
}

.MuiSelect-select:focus {
    background-color: transparent;
}

.AvatarUploadPos {
    position: relative;
    top: 20px;
}

.RacingIcons .MuiFormControlLabel-label {
    color: var(--black-light);
    font-size: 12px;
}

.RacingIcons .MuiSvgIcon-root {
    background: var(--white);
    border-radius: 50%;
    padding: 7px;
}

.carouselIcons .MuiRadio-root {
    padding: 4px;
}

.footerIcons .MuiBottomNavigationAction-root {
    min-width: 77px;
}

.RacingTabs .MuiTabScrollButton-root {
    width: 30px;
}

.RacingTabs .MuiTabs-scrollButtons .MuiSvgIcon-root {
    font-size: 20px;
}

.floatRight {
    float: right !important;
}

.RacingTabButton {
    border-radius: 30px;
    padding: 8px 11px;
    min-height: 20px;
    margin-right: 5px;
    color: #000;
    display: flex;
    text-align: center;
    font-size: 20px;
    border: 0;
    box-shadow: none !important;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
}

.RacingTabButton:active,
.RacingTabButton:focus {
    box-shadow: none !important;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
}

.BetProductSelect {
    border-radius: 20px;
    font-size: 13px;
}

.BetProductSelect .MuiSelect-select {
    padding: 3px 20px 3px 15px;
}

.FavBadge .MuiBadge-badge {
    right: 50%;
    border-radius: 5px;
    font-weight: bold;
    padding: 0 10px;
    top: -8px;
    font-size: 10px;
    z-index: 0;
    border: 1px solid grey;
    height: 16px;
}

.successCheckbox {
    border: 1px solid #cccccc;
    border-radius: 6px;
    font-size: 30px;
}

.HeaderBadge .MuiBadge-badge {
    min-width: auto;
    height: auto;
    font-size: 9px;
    padding: 2.5px;
}

.customKeypad {
    border-radius: 4px;
    display: flex;
    justify-content: center;
    align-items: center;
    height: 45px;
}

.MuiAccordionSummary-root.Mui-expanded {
    min-height: auto;
}

.MuiAccordionSummary-content.Mui-expanded {
    margin: 10px 0;
}

.MuiCardHeader-avatar {
    margin-right: 10px;
}

.custombtn {
    background-color: #3f3f3f;
    /* border: 1px solid #363636; */
    border-radius: 7px;
    color: #fff;
    height: 40px;
}

.btnError {
    border: 2px solid #f60406
}

.custombtn .MuiOutlinedInput-root {
    color: #fff;
    height: 100%;
}

.custombtn .Mui-focused {
    background-color: #fff;
    color: #000;
}

.custombtn .Mui-focused .MuiInputAdornment-root .MuiTypography-root {
    color: #000;
}

.custombtn .MuiOutlinedInput-input {
    margin: 0;
    padding: 0;
    height: 50%;
    margin-right: 10px;
}

.custombtn .Mui-focused .MuiOutlinedInput-input {
    animation-duration: 1s;
    animation-iteration-count: infinite;
    animation-name: customCursor;
    animation-timing-function: step-end;
    border-right: 1px solid #fff;
    border-radius: 0;
}

.custombtn .MuiOutlinedInput-notchedOutline {
    border: 0;
    top: 0;
}

.resultedRaceTable .MuiGrid-root:not(:first-child) {
    pointer-events: none;
}

.up-arrow {
    width: 0;
    height: 0;
    border: solid 4px transparent;
    background: transparent;
    border-bottom: solid 5px grey;
    border-top-width: 0;
    cursor: pointer;
}

.down-arrow {
    width: 0;
    height: 0;
    border: solid 4px transparent;
    background: transparent;
    border-top: solid 5px grey;
    border-bottom-width: 0;
    margin-top: 1px;
    cursor: pointer;
}

.BetProductsSelect:after,
.BetProductsSelect:before,
.BetProductsSelect:hover:after,
.BetProductsSelect:hover:before {
    border-bottom: 0 !important;
}

.MuiSnackbarContent-root {
    padding: 0;
}

.MuiSnackbarContent-message {
    padding: 0;
    width: 100%;
}

.RacemeetSelect .MuiSelect-select {
    padding-right: 2px !important;
}

.RacemeetSelect .MuiSelect-select span {
    font-size: 16px;
}

.HideTextOverflow {
    overflow: hidden;
    text-overflow: ellipsis;
}

@keyframes customCursor {

    0%,
    100% {
        border-color: #000;
    }

    40% {
        border-color: #ffffff;
    }
}

/* image uploader styles */

.img-uploader-preview-image {
    width: 70;
    height: 70;
}

.img-uploader-title {
    text-align: center;
    font-weight: bold;
    padding: 0;
    margin: 0;
}

.img-uploader-sub-title {
    text-align: center;
    font-weight: 200;
    margin: 0;
}

.img-uploader-error {
    text-align: center;
    color: red;
}

.img-preview-title {
    text-align: center !important;
    padding: 0 !important;
    margin: 0 !important;
}

.img-preview-sub-title {
    text-align: center !important;
    color: red;
    padding: 0;
    margin: 0;
}

.sub-accordin {
    border: none !important;
    box-shadow: none !important;
    margin: 0 !important;
    padding: 0 !important;
}

.form-wrapper {
    height: inherit;
    overflow: scroll;
    padding: 2%;
}

.form-container {
    background-color: white;
    border: 2px solid rgb(255, 255, 255);
    padding: 5%;
    border-radius: 15px;
}

.fieldText {
    color: var(--black);
}

.MuiMenu-paper {
    transition-duration: 0 !important;
}

/* ---------sport page */
.data-row-tilte-text {
    margin: 0px;
    font-family: Roboto, Helvetica, Arial, sans-serif;
    font-weight: 400;
    font-size: 1rem;
    line-height: 1.5;
    letter-spacing: 0.00938em;
    color: rgb(0, 0, 0);
    font-size: 14px !important;
    text-transform: capitalize !important;
    line-height: 1 !important;
}

.data-row-sub-tilte-text {
    margin: 0px;
    font-family: Roboto, Helvetica, Arial, sans-serif;
    font-weight: 400;
    line-height: 1.5;
    letter-spacing: 0.00938em;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: rgb(102, 102, 102);
    font-size: 12px;
    overflow-wrap: break-word;
}